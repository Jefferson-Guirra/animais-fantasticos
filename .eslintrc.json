{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions":0,
        "no-param-reassign":0,
        "no-console":0,
        "max-classes-per-file":0,
        "no-return-assign":0
        
        
    }
}
